#include <iostream>
using namespace std;


bool isPrimo(int num) {
    if (num <= 1) return false; 
    for (int i = 2; i * i <= num; i++) {
        if (num % i == 0) {
            return false; 
        }
    }
    return true;
}

int main() {
    cout << "NÃºmeros primos entre 1 e 100:" << endl;

    for (int i = 1; i <= 100; i++) {
        if (isPrimo(i)) {
            cout << i << " ";
        }
    }
    cout << endl;

    return 0;
}
